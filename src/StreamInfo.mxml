			private var streamTempTooltip:String = "<b>July mean:</b> July daily mean stream temperature (Late 20th century values were predicted using observed values; Mid and Late 21st century values were predicted using downscaled climate projections from 13 general circulation models)<br/><b>Thermal class:</b> classified by July mean stream temperature (in degrees celsius), where Cold = &lt; 17.5째C; Cold transition = 17.5 - 19.5째C; Warm transition = 19.5 - 21째C; and Warm Water = &gt; 21째C)<br/><b>Change in thermal class:</b> Comparison of present thermal class to future time period thermal class<br/><b>Change in degrees:</b> Difference in July mean stream temperature between present and future time period (in degrees Farenheit)";
			private var fishSummaryTooltip:String = "<p><b>Predicted occurrence:</b> Indicates whether a fish thermal group or species are present or absent (where predicted probability of occurrence &gt; = 50%, species were classified as being present; otherwise species were classified as being absent)</p><br/><p><b>Number of species lost:</b> Number of species lost in a future time period compared to the present</p><br/><p><b>Number of species gained:</b> Number of species gained in a future time period compared to the present</p><br/><p><b>Number of species lost or gained:</b> Total number of species lost and gained in a future time period compared to the present</p><br/><p><b>Percent of species lost:</b> Percent of species lost in a future time period compared to the present</p><br/><p><b>Percent of species gained:</b> Percent of species gained in a future time period compared to the present</p>"
			private var fishIndSpeciesTooltip:String = "<p><b>Predicted occurrence:</b> Indicates whether a fish thermal group or species are present or absent (where predicted probability of occurrence &gt; = 50%, species were classified as being present; otherwise species were classified as being absent)";
			private var streamflowTooltip:String = "<p><b>Annual Q50 flow:</b> Streamflow is at least this high 50% of the year (medium flow). Measured in cubic feet per second.</p><p><b>Annual Q50 yield:</b> Annual Q50 streamflow (medium flow) divided by drainage area. Measured in cubic feet per second per square mile.</p><p><b>August Q90 flow:</b> Streamflow is at least this high 90% of the year (low flow). Measured in cubic feet per second.</p><p><b>August Q90 yield:</b> August Q90 streamflow (low flow) divided by drainage area. Measured in cubic feet per second per square mile.</p>";
			private var climateAvgTooltip:String = "<p><b>Mean annual air temperature:</b> Average yearly temperature (Late 20th century, Mid, and Late 21st century values were projected from downscaled climatedata from 13 general circulation models (GCMs))</p><p><b>Change in mean annual air temperature:</b> Difference in average yearly temperature/precipitation between present and future time period</p><p><b>Mean annual precipitation:</b> Average yearly precipitation (Late 20th century, Mid, and Late 21st century values were projected from downscaled climate data from 13 general circulation models (GCMs))</p><p><b>Change in mean annual precipitation:</b> Difference in average yearly precipitation between present and future time period</p>";
			private var climateRawTooltip:String = "<p><b>Mean annual air temperature:</b> Average yearly temperature (Late 20th century, Mid, and Late 21st century values were projected from downscaled climate data from one global circulation model (GCM))</p><p><b>Change in mean annual air temperature:</b> Difference in average yearly temperature/precipitation between present and future time period</p><p><b>Mean annual precipitation:</b> Average yearly precipitation (Late 20th century, Mid, and Late 21st century values were projected from downscaled climate data from one global circulation model (GCM))</p><p><b>Change in mean annual precipitation:</b> Difference in average yearly precipitation between present and future time period</p>";
			
			protected function componentLoad():void
			{
				zoomToFeaturesOnMap.addEventListener(MouseEvent.CLICK, zoomToFeatures);
				zoomToFeaturesOnMap.visible = true;
				zoomToFeaturesOnMap.includeInLayout = true;
				
				if (data[0].state == "catchments") {
					currentState = "catchments";
				} else if (data[0].state == "streams") {
					currentState = "streams";
				}
				
				//logic to open the right tab for the mapped variable
				var val:String = FlexGlobals.topLevelApplication.topicOpt.selectedValue;
				if (val == "fish") {
					infoTabs.selectedIndex = 1;
				} else if (val == "streamtemp") {
					infoTabs.selectedIndex = 0;
				} else if (val == "streamflow") {
					infoTabs.selectedIndex = 3;
				} else if (val == "climateproj" && currentState == "catchments") {
					infoTabs.selectedIndex = 4;
				}
			}
			
			protected function zoomToFeatures(event):void {
				var graphicProvider:ArrayCollection = FlexGlobals.topLevelApplication.queryGraphicsLayer.graphicProvider as ArrayCollection;
				if (graphicProvider.length > 0) {
					var graphicsExtent:Extent = GraphicUtil.getGraphicsExtent(graphicProvider.toArray());
					FlexGlobals.topLevelApplication.map.extent = graphicsExtent.expand(3.1);
				}
			}
			
			protected function infoFault(info:Object, token:Object = null):void
			{
				Alert.show(info.toString());
			} 
			
			private function queryFault(info:Object, token:Object = null):void
			{
				Alert.show(info.toString());
			} 
			
			public function checkValue(attr:String):Number {
				var numb:Number;
				if (attr == '9999' || attr == null) {
					numb = 0;
				} else {
					numb = Number(attr);
				}
				
				return numb;
			}
			
			private function speciesClick(event:MouseEvent) {
				indFishResponseContent.visible = true;
				
				fishResponse.speciesName = event.currentTarget.speciesName;
				fishResponse.speciesNumber = event.currentTarget.speciesNumber;
				fishResponse.tempClass = event.currentTarget.tempClass;
				fishResponse.tempClassColor = event.currentTarget.tempClassColor;
				fishResponse.dataObj = event.currentTarget.dataObj;
				
				fishResponse.updated(event);
			}
			
			private function roundValues(fieldValue:String,decPlaces:Number):String {
				var outputValue:String;
				
				outputValue = ((Number(fieldValue)).toFixed(decPlaces)).toString()
					
				if (decPlaces == 0 && outputValue.split('.') != null) {
					outputValue = outputValue.split('.')[0];
				}
				
				return outputValue;
			}
			
			private function updateClassChange(fieldValue:String):String {
				var outputString = '';
				
				outputString = fieldValue.replace("_", " to ");
				
				return outputString;
			}
			
			private function convertToFah(celsiusValue:String):String {
				var outputFah = '';
				
				outputFah = ((Number(celsiusValue)*1.8) + 32).toFixed(1).toString();
				
				return outputFah;
			}
			
		]]>
